{
    "name": "laravel/laravel",
    "type": "project",
    "version": "1.0.0",
    "description": "",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "spatie/laravel-permission": "^6.9",
        "stevebauman/location": "^7.3",
        "yajra/laravel-datatables": "11.0",
        "yajra/laravel-datatables-oracle": "11.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "barryvdh/laravel-debugbar": "^3.13.5",
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.2.7",
        "spatie/laravel-ignition": "^2.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Permissions\\": "app/Modules/Permissions",
            "Settings\\": "app/Modules/Settings",
            "General\\": "app/Modules/General",
            "Languages\\": "app/Modules/Languages",
            "Admins\\": "app/Modules/Admins",
            "Contacts\\": "app/Modules/Contacts"
        },
        "files": [
            "app/Helpers/app.php",
            "app/Helpers/module.php",
            "app/Helpers/theme.php",
            "app/Modules/Settings/Helpers/settings.php",
            "app/Modules/Languages/Helpers/language.php",
            "app/Modules/Admins/Helpers/admin.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.2"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
